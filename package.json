{
  "name": "consultacep",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://diegoroso.github.io/consultacep",
  "dependencies": {
    "axios": "^0.18.0",
    "gh-pages": "^2.0.1",
    "google-map-react": "^1.1.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-app-polyfill": "^0.2.0",
    "react-dom": "^16.7.0",
    "react-input-mask": "^2.0.4",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "23.6.0",
    "cypress": "^3.1.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "fetch-mock": "^7.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.6.0",
    "node-fetch": "^2.3.0",
    "redux-devtools-extension": "^2.13.7",
    "redux-mock-store": "^1.5.3",
    "sass-lint": "^1.12.1"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.[j]s?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "moduleNameMapper": {
      "\\.(png|gif)$": "<rootDir>/src/assets/__mocks__/fileMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    }
  }
}
